{
  "name": "PlaceAVote",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "xcode": "open ios/PlaceAVote.xcodeproj",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "setup": "npm install -g rnpm && npm install && rnpm link && rnpm link assets && brew install carthage && cd ios && carthage update --platform iOS",
    "test": "rm -rf ./node_modules/jest-cli/.haste_cache && jest  --no-cache",
    "pavclient": "npm i git+https://a7ee7e677992613cade67c796347ad55e3a0ab64:x-oauth-basic@github.com/PlaceAVote/pav-client-sdk.git#master",
    "releaseAndroid": "cd android && ./gradlew assembleRelease",
    "clean": "watchman watch-del . ; watchman watch-project ."
  },
  "author": {
    "name": "Ioannis Kokkinidis",
    "email": "ioannis@placeavote.com",
    "url": "https://www.linkedin.com/in/sudoplz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PlaceAVote/pav-mobile"
  },
  "bugs": {
    "url": "https://github.com/PlaceAVote/pav-mobile/issues",
    "email": "ioannis@placeavote.com"
  },
  "engines": {
    "node": ">=5.0",
    "npm": ">=3.0"
  },
  "jest": {
    "scriptPreprocessor": "./node_modules/react-native/jestSupport/preprocessor.js",
    "setupEnvScriptFile": "./node_modules/react-native/jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-addons-test-utils",
      "react-native-router-flux",
      "promise",
      "source-map",
      "key-mirror",
      "immutable",
      "fetch",
      "redux",
      "redux-thunk",
      "fbjs",
      "underscore",
      "validate.js",
      "moment"
    ],
    "collectCoverage": false,
    "verbose": true
  },
  "rnpm": {
    "assets": [
      "assets/fonts"
    ]
  },
  "devDependencies": {
    "jest-cli": "^0.9.2",
    "remote-redux-devtools": "^0.3.3"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "key-mirror": "SudoPlz/key-mirror",
    "moment": "^2.12.0",
    "pavclient": "git+https://a7ee7e677992613cade67c796347ad55e3a0ab64:x-oauth-basic@github.com/PlaceAVote/pav-client-sdk.git#master",
    "react": "15.1.0",
    "react-addons-test-utils": "15.1.0",
    "react-native": "0.27.2",
    "react-native-bugsnag": "0.0.4",
    "react-native-collapsible": "^0.6.0",
    "react-native-fbsdk": "^0.2.2",
    "react-native-google-analytics-bridge": "^2.1.0",
    "react-native-image-picker": "^0.19.6",
    "react-native-image-progress": "SudoPlz/react-native-image-progress",
    "react-native-keyboard-spacer": "SudoPlz/react-native-keyboard-spacer",
    "react-native-linear-gradient": "1.5.12",
    "react-native-modalbox": "^1.3.3",
    "react-native-orientation": "^1.16.0",
    "react-native-progress": "^3.0.0",
    "react-native-router-flux": "3.30.0",
    "react-native-scrollable-tab-view": "^0.4.4",
    "react-native-simple-store": "^1.0.1",
    "react-native-vector-icons": "^2.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^1.0.3",
    "sp-react-native-iconbutton": "^2.4.13",
    "tcomb-form-native": "0.5.1",
    "underscore": "^1.8.3",
    "validate.js": "^0.9.0",
    "react-native-swiper": "1.4.6"
  }
}
