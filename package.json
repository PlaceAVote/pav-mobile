{
  "name": "PlaceAVoteApp",
  "version": "0.0.1",
  "private": true,
  "author":{
    "name" : "Ioannis Kokkinidis",
    "email" : "ioannis@placeavote.com",
    "url" : "https://www.linkedin.com/in/sudoplz"
  },
  "repository" :{
    "type" : "git",
    "url" : "https://github.com/PlaceAVote/pav-mobile"
  },
  "bugs":  {
    "url" : "https://github.com/PlaceAVote/pav-mobile/issues",
    "email" : "ioannis@placeavote.com"
  },
  "scripts": {
    "start": "npm run serve",
    "serve": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "xcode": "open ios/PlaceAVoteApp.xcodeproj/",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "setup": "npm install -g npm3 && npm3 install -g rnpm && npm3 install && rnpm link",
    "test": "rm -rf ./node_modules/jest-cli/.haste_cache && jest  --no-cache"
  },
  "dependencies": {
    "react": "^0.14.8",
    "react-native": "^0.22.2",
    "react-native-router-flux": "^3.2.4",
    "react-native-vector-icons": "^1.1.0",
    "react-native-simple-store": "^0.1.0",
    "redux": "^3.1.2",
    "redux-thunk": "^1.0.3",
    "react-redux": "^4.1.1",
    "immutable": "^3.7.6",
    "underscore": "^1.8.3",
    "validate.js": "^0.9.0",
    "key-mirror": "^1.0.1",
    "regenerator": "^0.8.42",
    "sp-react-native-iconbutton": "^2.4.6",
    "react-native-gifted-spinner": "0.0.3",
    "tcomb-form-native": "^0.4.0",
    "react-native-orientation": "^1.15.0",
    "react-native-collapsible": "^0.5.6",
    "moment": "^2.12.0",
    "react-native-modalbox": "^1.3.0"

  },
  "devDependencies":{
    "babel-core": "^6.7.4",
    "babel-jest": "^9.0.3",
    "jest-cli": "^0.9.2",
    "remote-redux-devtools":"0.1.5",
    "react-addons-test-utils": "^0.14.7",
    "redux-mock-store": "^1.0.2"
  },

  "rnpm": {
    "assets": [
      "assets/fonts"
    ]
  },

  "jest": {
    "scriptPreprocessor": "./node_modules/react-native/jestSupport/preprocessor.js",
    "setupEnvScriptFile": "./node_modules/react-native/jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-addons-test-utils",
      "react-native-router-flux",
      "promise",
      "source-map",
      "key-mirror",
      "immutable",
      "fetch",
      "redux",
      "redux-thunk",
      "fbjs",
      "underscore",
      "validate.js",
      "moment"
    ],
    "collectCoverage": false,
    "verbose": true
  }
}
